{
    "extends": ["tslint:recommended", "tslint-react"],
    "defaultSeverity": "warning",
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
      "interface-name": false,
      "max-line-length": false,
      "jsx-no-lambda": false
    },    
    "jsRules": {
      "no-empty": true
    }
}
